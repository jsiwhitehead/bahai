{
  ~'value':: '',
  : [
    'size': 20,
    'font': 'Atkinson Hyperlegible',
    'color': '#333',
    'pad': 50,
    'style': ['max-width': '800px', 'margin': '0 auto'],
    [
      'align': 'center',
      'size': 40,
      'todos',
    ],
    ['input': 'text', 'fill': 'lightblue', 'value':=],
    [value],
  ]
}